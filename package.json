{
  "name": "enable365-theme",
  "version": "1.0.0",
  "main": "index.js",
  "scripts": {
    "build:css": "tailwindcss -i ./src/input.css -o ./style.tailwind.css",
    "build:editor": "tailwindcss -c ./tailwind.config.editor.js -i ./src/editor.css -o ./style.editor.css",
    "watch:css": "tailwindcss -i ./src/input.css -o ./style.tailwind.css --watch",
    "build": "npm run build:css && npm run build:editor"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "description": "Enable365 WordPress Theme",
  "devDependencies": {
    "autoprefixer": "^10.4.21",
    "postcss": "^8.5.6",
    "tailwindcss": "^3.4.17"
  }
}
